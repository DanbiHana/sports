<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mbc.sports.board.BoardService">
	<select id="out">
		select *
		from mvpboard
		order by step
	</select>
	<insert id="insert">
		insert into mvpboard
		values (mvpnum_seq.nextval(),#{param1},#{param2},#{param3},#{param4},#{param5},0)
	</insert>
	
</mapper>